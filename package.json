{
  "name": "fastify-demo",
  "version": "0.0.0",
  "description": "The official Fastify demo!",
  "main": "app.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run db:seed && tap --jobs=1 test/**/*",
    "start": "npm run build && fastify start -l info dist/app.js",
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "standalone": "node --env-file=.env dist/server.js",
    "lint": "eslint --ignore-pattern=dist",
    "lint:fix": "npm run lint -- --fix",
    "db:migrate": "node --env-file=.env scripts/migrate.js",
    "db:seed": "node --env-file=.env scripts/seed-database.js"
  },
  "keywords": [],
  "author": "Michelet Jean <jean.antoine.michelet@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@fastify/autoload": "^5.10.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.3.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/mysql": "^4.3.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@fastify/under-pressure": "^8.3.0",
    "@sinclair/typebox": "^0.32.31",
    "concurrently": "^8.2.2",
    "fastify": "^4.26.1",
    "fastify-cli": "^6.1.1",
    "fastify-plugin": "^4.0.0",
    "postgrator": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.4.0",
    "fastify-tsconfig": "^2.0.0",
    "mysql2": "^3.10.1",
    "neostandard": "^0.7.0",
    "tap": "^19.2.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
