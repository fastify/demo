{
  "name": "fastify-demo",
  "version": "0.0.0",
  "description": "The official Fastify demo!",
  "main": "app.js",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm run build && fastify start -l info dist/app.js",
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch\" \"npm:dev:start\"",
    "build:client": "NODE_ENV=production vite build",
    "dev:start": "npm run build && fastify start --ignore-watch='.ts$ src/client' -w -l info -P dist/app.js",
    "test": "npm run db:seed && c8 npm run test:run",
    "test:run": "glob -c \"tsx --test\" \"./test/**/*.ts\"",
    "standalone": "npm run build && node --env-file=.env dist/server.js",
    "lint": "eslint --ignore-pattern=dist",
    "lint:fix": "npm run lint -- --fix",
    "db:create": "tsx --env-file=.env ./scripts/create-database.ts",
    "db:drop": "tsx --env-file=.env ./scripts/drop-database.ts",
    "db:migrate": "tsx --env-file=.env ./scripts/migrate.ts",
    "db:seed": "tsx --env-file=.env ./scripts/seed-database.ts"
  },
  "keywords": [],
  "author": "Michelet Jean <jean.antoine.michelet@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastify/demo.git"
  },
  "homepage": "https://github.com/fastify/demo#readme",
  "bugs": {
    "url": "https://github.com/fastify/demo/issues"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/react": "^1.1.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/session": "^11.1.0",
    "@fastify/static": "^8.1.1",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@fastify/under-pressure": "^9.0.3",
    "@fastify/vite": "^8.1.3",
    "@radix-ui/react-accordion": "^1.2.11",
    "@sinclair/typebox": "^0.34.33",
    "@tailwindcss/vite": "^4.1.7",
    "@unhead/react": "^2.0.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "csv-stringify": "^6.5.2",
    "fastify": "^5.2.2",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.1",
    "history": "^5.3.0",
    "knex": "^3.1.0",
    "lucide-react": "^0.511.0",
    "minipass": "^7.1.2",
    "mysql2": "^3.14.0",
    "postgrator": "^8.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0",
    "sanitize-filename": "^1.6.3",
    "tailwind-merge": "^3.3.0",
    "tw-animate-css": "^1.3.0",
    "valtio": "^2.1.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "c8": "^10.1.3",
    "eslint": "^9.24.0",
    "fastify-tsconfig": "^3.0.0",
    "form-data": "^4.0.2",
    "glob": "^11.0.1",
    "neostandard": "^0.12.1",
    "oxlint": "^0.16.11",
    "postcss": "^8.5.3",
    "postcss-preset-env": "^10.1.6",
    "tailwindcss": "^4.1.7",
    "tsx": "^4.19.3",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  }
}
